{
  "name": "fen-pdf",
  "version": "1.0.0",
  "description": "open source package for browser based viewing and editing of PDFs",
  "scripts": {
    "dev": "webpack --mode development --config webpack.dev.js",
    "prod": "webpack --mode production --config webpack.prod.js",
    "serve": "webpack-dev-server --open --config webpack.dev.js --mode development",
    "test": "karma start --single-run --colors false karma.config.js",
    "watch-test": "karma start karma.config.js",
    "lint": "./node_modules/eslint/bin/eslint.js ./src/**/*.js",
    "style-lint": "./node_modules/stylelint/bin/stylelint.js ./scss/**/*.scss --syntax scss"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/desertrat-io/fen-pdf"
  },
  "keywords": [
    "pdf",
    "viewer",
    "editing",
    "edit",
    "redaction",
    "markup",
    "zoom",
    "rotate",
    "pdfviewer",
    "adobe",
    "desertrat-io"
  ],
  "private": true,
  "author": "Mike Lawson",
  "email": "mike@desertrat.io",
  "license": "ISC",
